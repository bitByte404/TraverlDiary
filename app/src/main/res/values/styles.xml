<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TransparentBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
    <item name="android:backgroundDimEnabled">true</item>
    <item name="android:backgroundDimAmount">0.5</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:colorBackgroundCacheHint">@null</item>
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:statusBarColor">@android:color/transparent</item>
    <item name="android:navigationBarColor">@android:color/transparent</item>
</style>
    <style name="NoPaddingBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
    <item name="android:windowBackground">@android:color/transparent</item>
</style>
    <style name="MyFloatingActionButton" parent="Widget.MaterialComponents.FloatingActionButton">
    <item name="colorOnPrimary">@color/white</item>
</style>
</resources>